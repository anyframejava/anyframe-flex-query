<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	
	<bean id="flexService" class="org.anyframe.flex.query.service.impl.FlexServiceImpl">
		<property name="flexDao" ref="flexDao"/>
	</bean>
	
	<bean id="flexDao" class="org.anyframe.flex.query.dao.FlexDao">
		<property name="queryService" ref="queryService"/>
		<property name="findPrefix" value="flex.find"/>
		<property name="createId" value="flex.create"/>
		<property name="removeId" value="flex.remove"/>
		<property name="updateId" value="flex.update"/>
	</bean>
</beans>
